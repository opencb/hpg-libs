import os

Import('bioenv', 'debug')

formats_env = bioenv.Clone()
formats_env['objects'] = []


# Targets
formats = ['fastq', 'bam', 'bed']

for dir in formats:
    SConscript(os.path.join(dir, 'SConscript'), exports = ['bioenv', 'formats_env', 'debug'])

bioenv['objects'] += formats_env['objects']
